package utils;


public class Sudoku {
	
	private static String[][] valori;
	
	private static void stampaSudokuErr() {
		for (int i = 0; i < 9; i++) {
			for (int j = 0; j < 9; j++) {
				System.out.print(valori[i][j]+" ");
				if(j==2 || j==5){
					System.out.print("| ");
				}
				
			}
			System.out.println(" ");
			if(i==2 || i==5){
				for (int h = 0; h < 22; h++) {
					System.out.print("-");
				}
				System.out.println(" ");
			}	
		}
	}
}
